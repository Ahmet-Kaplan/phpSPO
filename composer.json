{
    "name": "vgrem/phpSPO",
    "description": "The library provides a SharePoint REST client for PHP applications. It allows to performs CRUD operations against SharePoint resources via an REST/OData based API",
    "type": "library",
    "keywords": ["sharepoint", "office365", "rest", "curl", "api"],
    "authors": [
        {
            "name": "Vadim Gremyachev",
            "email": "vvgrem@gmail.com"
        }
    ],
    "license": "LGPL-3.0+",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.3",
        "ext-curl": "*"
    },
    "autoload": {
        "psr-4": {
            "SharePoint\\PHP\\Client\\": ["src/", "src/auth", "src/utilities", "src/runtime", "src/userprofiles", "src/taxonomy"]
        }
    }
}

