{
    "name": "vgrem/php-spo",
    "description": "Office 365 library for PHP. It allows to performs CRUD operations against Office 365 resources via an REST/OData based API",
    "type": "library",
    "keywords": [
        "office365",
        "rest",
        "curl",
        "api"
    ],
    "authors": [
        {
            "name": "Vadim Gremyachev",
            "email": "vvgrem@gmail.com"
        }
    ],
    "license": "LGPL-3.0+",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-simplexml": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7"
    },
    "autoload": {
        "psr-4": {
            "Office365\\PHP\\Client\\": "src/"
        }
    }
}
